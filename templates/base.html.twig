<!DOCTYPE html>
<html lang="{{ app.request.locale|split('_')[0] }}" {% block html_attributes '' %}>
    <head {% block head_attributes '' %}>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
        <title>{% block title %}Imbrication de formulaire{% endblock title %}</title>
        {% block metadatas '' %}
        {% block icon '' %}
        {% block stylesheets %}
            {{ encore_entry_link_tags('base') }}
        {% endblock stylesheets %}
    </head>
    <body>
        <main class="content">
            {% block content '' %}
        </main>
        {% block javascripts %}
            {{ encore_entry_script_tags('base') }}
        {% endblock javascripts %}
    </body>
</html>
